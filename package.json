{
  "name": "unhttp",
  "version": "0.0.0",
  "type": "module",
  "description": "write (your request handlers) once, run anywhere",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./adapters/*": {
      "types": "./dist/adapters/*/index.d.ts",
      "import": "./dist/adapters/*/index.js",
      "default": "./dist/adapters/*/index.cjs"
    }
  },
  "files": ["dist"],
  "scripts": {
    "build": "rimraf dist && run-p build:*",
    "build:js": "tsup",
    "build:types": "tsc -p tsconfig.build.json",
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "lint:js": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "peerDependencies": {
    "express": "^4",
    "fastify": "^3",
    "h3": "*",
    "koa": "^2"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/koa": "2.13.4",
    "@types/node": "17.0.36",
    "@typescript-eslint/eslint-plugin": "5.26.0",
    "@typescript-eslint/parser": "5.26.0",
    "dprint": "0.29.1",
    "esbuild-node-loader": "0.8.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "express": "4.18.1",
    "fastify": "3.29.0",
    "get-port": "6.1.2",
    "h3": "0.7.8",
    "husky": "8.0.1",
    "koa": "2.13.4",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "rimraf": "3.0.2",
    "tsup": "6.0.1",
    "typescript": "4.7.2",
    "undici": "5.3.0",
    "vite": "2.9.9",
    "vitest": "0.12.9"
  },
  "sideEffects": false,
  "nano-staged": {
    "*.{js,ts,md,json,yaml}": "dprint fmt"
  },
  "tsup": {
    "entry": ["src/index.ts", "src/adapters/*/index.ts"],
    "format": ["esm", "cjs"],
    "splitting": true
  }
}
