{
  "name": "unhttp",
  "version": "0.0.0",
  "type": "module",
  "description": "write (your request handlers) once, run anywhere",
  "main": "dist/index.cjs",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "import": "./dist/index.js",
      "default": "./dist/index.cjs"
    },
    "./adapters/node": {
      "import": "./dist/adapters/node.js",
      "default": "./dist/adapters/node.js"
    },
    "./adapters/fetch": {
      "import": "./dist/adapters/fetch.js",
      "default": "./dist/adapters/fetch.js"
    }
  },
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "format": "dprint fmt",
    "lint": "run-p lint:*",
    "lint:format": "dprint check",
    "lint:types": "tsc",
    "lint:js": "eslint .",
    "prepare": "husky install",
    "prepublishOnly": "pnpm build",
    "test": "vitest"
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/koa": "2.13.4",
    "@types/node": "17.0.29",
    "@typescript-eslint/eslint-plugin": "5.21.0",
    "@typescript-eslint/parser": "5.21.0",
    "dprint": "0.27.0",
    "esbuild-node-loader": "0.8.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "express": "4.18.1",
    "fastify": "3.29.0",
    "get-port": "6.1.2",
    "h3": "0.7.8",
    "husky": "7.0.4",
    "koa": "2.13.4",
    "nano-staged": "0.8.0",
    "npm-run-all": "4.1.5",
    "nuxt": "3.0.0-rc.3",
    "tsup": "5.12.6",
    "typescript": "4.6.3",
    "undici": "5.3.0",
    "vite": "2.9.6",
    "vitest": "0.10.0"
  },
  "sideEffects": false,
  "nano-staged": {
    "*.{js,ts,md,json,yaml}": "dprint fmt"
  },
  "tsup": {
    "entry": [
      "src/index.ts",
      "src/adapters/node.ts",
      "src/adapters/fetch.ts"
    ],
    "format": [
      "esm",
      "cjs"
    ],
    "dts": {
      "resolve": true
    },
    "target": "node14",
    "splitting": true,
    "clean": true
  }
}
